{% extends "base.twig" %}

{% block content %}
  <div class="wrapper --full">
    <article class="post-type-{{post.post_type}}" id="{{post.post_type}}-{{post.ID}}">
      {{ post.content }}
      <nav class="works">
      {% for work in works %}
        <a class="works__link" href="{{work.link}}" alt="{{work.custom['main_description']}}">{{work.post_title}}</a>
      {% endfor %}

      <!-- {{works|print_r }}-->
    </ul>
    </article>
  </div>
{% endblock %}